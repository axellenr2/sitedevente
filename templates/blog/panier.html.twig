{% extends 'base.html.twig' %}
{% block body %}
        
    <section id="Panier">
    <br>
    <br>
        <h1> Panier </h1>
{% for lignePanier in app.user.panier.lignearticle %}
    <ul class="list-group list-group-flush">
        <li class="list-group-item"><h2>{{ lignePanier.lignesPanier.title }}</h2><p>Quantité : {{ lignePanier.quantitearticle }}</p><h3>{{ lignePanier.lignesPanier.prix }} €</h3><img src="{{ lignePanier.lignesPanier.image }}" alt="" height="150" width="100"></li>
    </ul>
    
{% endfor %}
<h3> Prix total :  {{ app.user.panier.getTotal() }} € </h3>

<h3> Votre commande sera livré à : </h3> <p>{{ app.user.getAdresse() }} </p><br>
<h3> Paiement : </h3> <a href="https://www.paypal.com/fr/home"><img src="data:image/png;base64,"   height="50" width="100" alt="coucou"/></a><br><br>
    


<a type="submit" href="{{ path('commande') }}" class="btn btn-info">Passer la commande</a>
    

</section>
{% endblock %}

